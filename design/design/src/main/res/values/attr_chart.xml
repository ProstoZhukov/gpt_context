    -->
    <!--region Базовые графики-->
    <!--Пустое представление (график без данных)-->
    <attr name="graphs_base_color_0" format="color|reference"/>
    <!--Типовое представление-->
    <attr name="graphs_base_color_1" format="color|reference"/>
