    <attr name="graphs_base_color_0_opacity_20" format="color|reference"/>
    <attr name="graphs_base_color_0_opacity_50" format="color|reference"/>

    <!--2. За текущий период-->
    <!--Данные на текущую дату/прогноз-->
    <attr name="graphs_extra_color_1" format="color|reference"/>
    <!--Заливка графика (градиент 50-20%)-->
    <attr name="graphs_base_color_1_opacity_20" format="color|reference"/>
    <attr name="graphs_base_color_1_opacity_50" format="color|reference"/>
    <attr name="graphs_extra_color_3" format="color|reference"/>
    <!--endregion Графики сравнения-->
