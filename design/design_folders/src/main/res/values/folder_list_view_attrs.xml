<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Атрибуты компонента разверного списка папок -->
    <declare-styleable name="FolderListView">
        <!-- Мод отображения компонента. Влияет на доступность прокрутки списка.
         По умолчанию компонент самостоятельный -->
        <attr name="FolderListView_viewMode" format="enum">
            <!-- Самостоятельное отображение -->
            <enum name="stand_alone" value="0"/>
            <!-- Вложенное отображение -->
            <enum name="nested" value="1"/>
        </attr>
    </declare-styleable>

</resources>