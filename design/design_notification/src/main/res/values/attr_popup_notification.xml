<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <attr name="sbisPopupNotificationInfoTheme" format="reference" tools:ignore="ResourceName"/>
    <attr name="sbisPopupNotificationSuccessTheme" format="reference" tools:ignore="ResourceName"/>
    <attr name="sbisPopupNotificationWarningTheme" format="reference" tools:ignore="ResourceName"/>
    <attr name="sbisPopupNotificationErrorTheme" format="reference" tools:ignore="ResourceName"/>
    <attr name="sbisPopupNotificationNotificationTheme" format="reference" tools:ignore="ResourceName"/>

    <declare-styleable name="SbisPopupNotification" tools:ignore="ResourceName">
        <attr name="sbisPopupNotificationBackground" format="color|reference"/>
        <attr name="sbisPopupNotificationTextColor" format="color|reference"/>
        <attr name="sbisPopupNotificationIconColor" format="color|reference"/>
        <attr name="sbisPopupNotificationCloseIconAlpha" format="float"/>
        <attr name="sbisPopupNotificationCloseIconColor" format="color|reference"/>
        <attr name="sbisPopupNotificationDividerColor" format="color|reference"/>
    </declare-styleable>

</resources>