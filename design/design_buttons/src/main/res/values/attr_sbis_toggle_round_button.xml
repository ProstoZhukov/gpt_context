<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Специфичные для кнопки с залипанием атрибуты-->
    <!-- Поддерживаются все атрибуты из SbisRoundButton-->
    <declare-styleable name="SbisToggleRoundButton">
        <!-- Шрифтовая иконка в состоянии выбрана-->
        <attr name="SbisRoundButton_iconSelected" format="string|reference"/>
        <!-- Цвет фона кнопки для состояния выбрана -->
        <attr name="SbisRoundButton_backgroundColorSelected" format="color|reference"/>
        <!-- Цвет иконки кнопки в выбранном состоянии -->
        <attr name="SbisRoundButton_iconColorSelected" format="color|reference"/>
    </declare-styleable>
</resources>