<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- SbisTitleView -->
    <attr name="designSbisTitleViewTheme" format="reference" tools:ignore="ResourceName"/>

    <declare-styleable name="DesignSbisTitleView" tools:ignore="ResourceName">
        <attr name="DesignSbisTitleView_titleColor" format="color"/>
        <attr name="DesignSbisTitleView_subtitleColor" format="color"/>
        <!--
        Должен ли отображаться статус активности у фото (согласно указанному в PhotoData uuid).
        По умолчанию - false.
        -->
        <attr name="DesignSbisTitleView_withActivityStatus" format="boolean"/>
    </declare-styleable>
</resources>