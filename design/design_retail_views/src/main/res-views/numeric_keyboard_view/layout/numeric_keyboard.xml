<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="ResourceName"
    tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout"
    tools:theme="@style/RetailViewsNumericKeyboardCustomizedTheme.RetailDark">

    <ru.tensor.sbis.design.buttons.SbisRoundButton
        android:id="@+id/retail_views_key_1"
        style="?retail_views_numeric_keyboard_button_style"
        app:SbisRoundButton_icon="@string/retail_views_numeric_key_1"
        app:layout_constraintEnd_toStartOf="@+id/retail_views_key_2"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"/>

    <ru.tensor.sbis.design.buttons.SbisRoundButton
        android:id="@+id/retail_views_key_2"
        style="?retail_views_numeric_keyboard_button_style"
        android:layout_marginStart="?offset_2xs"
        android:layout_marginEnd="?offset_2xs"
        app:SbisRoundButton_icon="@string/retail_views_numeric_key_2"
        app:layout_constraintEnd_toStartOf="@+id/retail_views_key_3"
        app:layout_constraintStart_toEndOf="@+id/retail_views_key_1"/>

    <ru.tensor.sbis.design.buttons.SbisRoundButton
        android:id="@+id/retail_views_key_3"
        style="?retail_views_numeric_keyboard_button_style"
        app:SbisRoundButton_icon="@string/retail_views_numeric_key_3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/retail_views_key_2"/>

    <ru.tensor.sbis.design.buttons.SbisRoundButton
        android:id="@+id/retail_views_key_4"
        style="?retail_views_numeric_keyboard_button_style"
        android:layout_marginTop="?offset_2xs"
        app:SbisRoundButton_icon="@string/retail_views_numeric_key_4"
        app:layout_constraintEnd_toEndOf="@+id/retail_views_key_1"
        app:layout_constraintStart_toStartOf="@+id/retail_views_key_1"
        app:layout_constraintTop_toBottomOf="@+id/retail_views_key_1"/>

    <ru.tensor.sbis.design.buttons.SbisRoundButton
        android:id="@+id/retail_views_key_5"
        style="?retail_views_numeric_keyboard_button_style"
        android:layout_marginStart="?offset_2xs"
        android:layout_marginEnd="?offset_2xs"
        app:SbisRoundButton_icon="@string/retail_views_numeric_key_5"
        app:layout_constraintBottom_toBottomOf="@+id/retail_views_key_4"
        app:layout_constraintEnd_toEndOf="@+id/retail_views_key_2"
        app:layout_constraintStart_toStartOf="@+id/retail_views_key_2"
        app:layout_constraintTop_toTopOf="@+id/retail_views_key_4"/>

    <ru.tensor.sbis.design.buttons.SbisRoundButton
        android:id="@+id/retail_views_key_6"
        style="?retail_views_numeric_keyboard_button_style"
        app:SbisRoundButton_icon="@string/retail_views_numeric_key_6"
        app:layout_constraintBottom_toBottomOf="@+id/retail_views_key_4"
        app:layout_constraintEnd_toEndOf="@+id/retail_views_key_3"
        app:layout_constraintStart_toStartOf="@+id/retail_views_key_3"
        app:layout_constraintTop_toTopOf="@+id/retail_views_key_4"/>

    <ru.tensor.sbis.design.buttons.SbisRoundButton
        android:id="@+id/retail_views_key_7"
        style="?retail_views_numeric_keyboard_button_style"
        android:layout_marginTop="?offset_2xs"
        app:SbisRoundButton_icon="@string/retail_views_numeric_key_7"
        app:layout_constraintEnd_toEndOf="@+id/retail_views_key_1"
        app:layout_constraintStart_toStartOf="@+id/retail_views_key_1"
        app:layout_constraintTop_toBottomOf="@+id/retail_views_key_4"/>

    <ru.tensor.sbis.design.buttons.SbisRoundButton
        android:id="@+id/retail_views_key_8"
        style="?retail_views_numeric_keyboard_button_style"
        android:layout_marginStart="?offset_2xs"
        android:layout_marginEnd="?offset_2xs"
        app:SbisRoundButton_icon="@string/retail_views_numeric_key_8"
        app:layout_constraintBottom_toBottomOf="@+id/retail_views_key_7"
        app:layout_constraintEnd_toEndOf="@+id/retail_views_key_2"
        app:layout_constraintStart_toStartOf="@+id/retail_views_key_2"
        app:layout_constraintTop_toTopOf="@+id/retail_views_key_7"/>

    <ru.tensor.sbis.design.buttons.SbisRoundButton
        android:id="@+id/retail_views_key_9"
        style="?retail_views_numeric_keyboard_button_style"
        app:SbisRoundButton_icon="@string/retail_views_numeric_key_9"
        app:layout_constraintBottom_toBottomOf="@+id/retail_views_key_7"
        app:layout_constraintEnd_toEndOf="@+id/retail_views_key_3"
        app:layout_constraintStart_toStartOf="@+id/retail_views_key_3"
        app:layout_constraintTop_toTopOf="@+id/retail_views_key_7"/>

    <ru.tensor.sbis.design.buttons.SbisRoundButton
        android:id="@+id/retail_views_bottom_extra_left_button"
        style="?retail_views_numeric_keyboard_button_style"
        android:layout_marginTop="?offset_2xs"
        app:layout_constraintEnd_toEndOf="@+id/retail_views_key_1"
        app:layout_constraintStart_toStartOf="@+id/retail_views_key_1"
        app:layout_constraintTop_toBottomOf="@+id/retail_views_key_7"/>

    <ru.tensor.sbis.design.buttons.SbisRoundButton
        android:id="@+id/retail_views_key_0"
        style="?retail_views_numeric_keyboard_button_style"
        android:layout_marginStart="?offset_2xs"
        android:layout_marginEnd="?offset_2xs"
        app:SbisRoundButton_icon="@string/retail_views_numeric_key_0"
        app:layout_constraintBottom_toBottomOf="@+id/retail_views_bottom_extra_left_button"
        app:layout_constraintEnd_toEndOf="@+id/retail_views_key_2"
        app:layout_constraintStart_toStartOf="@+id/retail_views_key_2"
        app:layout_constraintTop_toTopOf="@+id/retail_views_bottom_extra_left_button"/>

    <ru.tensor.sbis.design.buttons.SbisRoundButton
        android:id="@+id/retail_views_bottom_extra_right_button"
        style="?retail_views_numeric_keyboard_button_style"
        app:layout_constraintBottom_toBottomOf="@+id/retail_views_bottom_extra_left_button"
        app:layout_constraintEnd_toEndOf="@+id/retail_views_key_3"
        app:layout_constraintStart_toStartOf="@+id/retail_views_key_3"
        app:layout_constraintTop_toTopOf="@+id/retail_views_bottom_extra_left_button"/>

</merge>