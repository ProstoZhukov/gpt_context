<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Атрибуты компонента [MoneyInputField] -->
    <declare-styleable name="RetailViewsMoneyInputFieldAttrs" tools:ignore="ResourceName">
        <attr name="retail_views_money_input_editable_field_theme" format="reference"/>

        <attr name="retail_views_money_input_editable_field_bottomStrokeColor" format="reference|color"/>
        <attr name="retail_views_money_input_editable_field_textColor" format="reference|color"/>
        <attr name="retail_views_money_input_editable_field_disabledTextColor" format="reference|color"/>
        <attr name="retail_views_money_input_editable_field_hintColor" format="reference|color"/>

        <attr name="retail_views_money_input_editable_field_integerPartMaxLength" format="reference|integer"/>
        <attr name="retail_views_money_input_editable_field_editableViewText" format="reference|string"/>
        <attr name="retail_views_money_input_editable_field_editableViewHint" format="reference|string"/>
        <attr name="retail_views_money_input_editable_field_isHorizontalFadingEdgeEnabled" format="reference|boolean"/>

        <attr name="android:inputType"/>
        <attr name="android:maxLength"/>
        <attr name="android:textSize"/>
        <attr name="android:ellipsize"/>
        <attr name="android:selectAllOnFocus"/>
        <attr name="android:privateImeOptions"/>
        <attr name="android:enabled"/>
    </declare-styleable>

</resources>