<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.mandatory.RequiredUpdateActivity">

    <ru.tensor.sbis.design_dialogs.movablepanel.MovablePanel
        android:id="@+id/versioning_version_movable_panel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center_horizontal"
        android:clickable="true"
        android:focusable="true"
        app:MovablePanel_background="?backgroundColor"
        app:MovablePanel_contentContainerId="@+id/versioning_movable_content_view_container_id"/>

</FrameLayout>