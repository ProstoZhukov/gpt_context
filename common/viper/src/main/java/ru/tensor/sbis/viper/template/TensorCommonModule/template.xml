<?xml version="1.0"?>
<template
    name="Tensor VIPER Base Common Module"
    description="Создает классы для наполнения модуля"
    format="4">

    <category value="Other"/>

    <parameter
        name="Название модуля"
        constraints="nonempty"
        default="App"
        help="без слова common, соблюдайте регистр"
        id="moduleName"
        type="string"/>

    <globals file="globals.xml.ftl"/>
    <execute file="recipe.xml.ftl"/>

</template>
