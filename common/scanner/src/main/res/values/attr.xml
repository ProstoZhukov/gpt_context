<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DocumentFinderView">
        <attr name="finder_mode">
            <enum name="display" value="0"/>
            <enum name="crop" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="DocumentImageView">
        <attr name="touch_enabled" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="SnapshotButton">
        <attr name="ring_color" format="color"/>
        <attr name="circle_color" format="color"/>
    </declare-styleable>

</resources>