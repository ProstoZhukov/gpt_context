<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ru.tensor.sbis.design.topNavigation.view.SbisTopNavigationView
        android:id="@+id/crm_another_operator_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:SbisTopNavigation_showBackButton="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
    </ru.tensor.sbis.design.topNavigation.view.SbisTopNavigationView>

    <ru.tensor.sbis.crud3.ListComponentView
        android:id="@+id/crm_another_operator_list"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:ListComponentView_forceBackgroundColor="white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/crm_another_operator_toolbar"/>

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/crm_another_operator_overlay_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true" />

</androidx.constraintlayout.widget.ConstraintLayout>